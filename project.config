	rebase = group bayesnet-devs
	rebase = group user/Lukasz Kostyra (LKostyra)
	removeReviewer = group bayesnet-devs
	removeReviewer = group user/Lukasz Kostyra (LKostyra)
	submit = group bayesnet-devs
	submit = group user/Lukasz Kostyra (LKostyra)
[access "refs/heads/*"]
	read = group bayesnet-devs
	read = group user/Lukasz Kostyra (LKostyra)
	create = group bayesnet-devs
	create = group user/Lukasz Kostyra (LKostyra)
	push = group bayesnet-devs
	push = +force group user/Lukasz Kostyra (LKostyra)
	pushMerge = group bayesnet-devs
